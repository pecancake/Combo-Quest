    {
        effect_create(x,y,'slow',sprite_width/2,sprite_height/2)
        movement_speed=movement_speed_start/2
        slowed-=1
    }
    if stunned
    {
        effect_create(x,y,'stun',sprite_width/2,sprite_height/2)
        movement_speed=0
        stunned-=1
    }
}

if !stunned and !slowed{movement_speed=movement_speed_start}
