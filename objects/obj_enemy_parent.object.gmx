if (obj_host.time mod 15)=0
{
    if burning
    {
        effect_create(x,y,'burn',sprite_width/2,sprite_height/2)
        hp-=floor(obj_host.dmg*0.1) sad_draw(x,y,obj_host.dmg*0.1,c_orange,'-')
        burning-=1
        pass_burn()
    }
    if poisoned
    {
        effect_create(x,y,'pois',sprite_width/2,sprite_height/2)
        hp-=floor(obj_host.dmg*0.25) sad_draw(x,y,obj_host.dmg*0.25,c_lime,'-')
        poisoned-=1
    }
    if slowed
    {
        effect_create(x,y,'slow',sprite_width/2,sprite_height/2)
        movement_speed=movement_speed_start/2
        slowed-=1
    }
    if stunned
    {
        effect_create(x,y,'stun',sprite_width/2,sprite_height/2)
        movement_speed=0
        stunned-=1
    }
}
if !stunned and !slowed{movement_speed=movement_speed_start}
