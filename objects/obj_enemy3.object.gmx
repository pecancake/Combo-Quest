            <string>///STATE MACHINE
event_inherited()
if state=s.pause
{
    if path_exists(path){path_end() path_delete(path)}
    p_timer-=1
    m_timer=60
    tar_x=x+random_range(-200,200)
    tar_y=y+random_range(-200,200)
    if p_timer&lt;=0{state=s.move}
    heal_id=0
}
if state=s.move
{
    h_timer=60    
    m_timer-=1
    path_find('find_path',tar_x,tar_y)
    if m_timer&lt;=0{state=s.heal}
    width=1
}
if state=s.heal
{
    if path_exists(path){path_end() path_delete(path)}
    
    h_timer-=1
    p_timer=irandom_range(60,200)
    heal_id=find_low_hp()
    if (obj_host.time mod 30)=0 and heal_id
    {   
        heal_id.hp+=10
        sad_draw(heal_id.x,heal_id.y,10,c_yellow,'+')
    }
    if width=1{FadeIn=true FadeOut=false}
    if width=8{FadeIn=false FadeOut=true}
    if FadeIn=true{width+=0.15}
    if FadeOut=true{width-=0.15}
    
    if heal_id
    {draw_line_width_colour(x,y,heal_id.x,heal_id.y,width,c_yellow,c_yellow)}
    
    if h_timer&lt;=0{state=s.pause}
}
if state=s.idle
{
    if path_exists(path){path_end() path_delete(path)}
}
