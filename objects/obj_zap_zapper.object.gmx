            <string>///DAMAGE, LIGHTNING DRAWING AND DEATH
if !damage_lock
{
    for (i=0;i!=ds_list_size(list);i+=1)
    {
        ID=list[| i]
        if ID.object_index=obj_crate
        {
            if colour=c_red{ID.burning=10}
            if colour=c_blue{ID.burning=0}
            with(ID){decay_particles(x,y,sprite_index,1)}
            ID.tint=1
        }
        else
        {
            if colour=c_red{ID.burning=10}
            if colour=c_blue{ID.burning=0}
            if colour=c_lime{ID.poisoned=10}
            if colour=c_aqua{ID.slowed=20}
            if colour=c_purple{if ID.light_type=true{ID.hp-=obj_host.dmg*2 sad_draw(ID.x,ID.y,obj_host.dmg*2,c_white,'-')}else{ID.hp+=obj_host.dmg sad_draw(ID.x,ID.y,obj_host.dmg,c_lime,'+')}}
            else{ID.hp-=obj_host.dmg sad_draw(ID.x,ID.y,obj_host.dmg,c_white,'-')}
            ID.stunned=10
            with(ID){decay_particles(x,y,sprite_index,1)}
            ID.tint=1
        }
    }
    damage_lock=true    
}

for (i=0;i!=ds_list_size(list);i+=1)
{
    ID=list[| i]
    col(colour)
    if instance_exists(ID){repeat(1){draw_lightning(x,y,ID.x,ID.y,10,15,5,10)}}
}

death+=1
if death=30{instance_destroy()}
</string>
